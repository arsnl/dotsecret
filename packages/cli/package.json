{
  "name": "dotsecret",
  "version": "0.0.0-next.7",
  "description": "Create and share secret files seamlessly across devices, platforms, and team members",
  "homepage": "https://dotsecret.org/",
  "bugs": {
    "url": "https://github.com/arsnl/dotsecret/issues"
  },
  "repository": {
    "url": "https://github.com/arsnl/dotsecret.git"
  },
  "license": "MIT",
  "author": "arsnl",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "bin": {
    "dotsecret": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js,.jsx,.mjs,.cjs,.ts,.tsx,.mts,.cts",
    "type": "tsc --noEmit",
    "check": "pnpm lint && pnpm type",
    "fix": "pnpm lint --fix"
  },
  "dependencies": {
    "axios": "1.6.7",
    "commander": "12.0.0",
    "cosmiconfig": "9.0.0",
    "dayjs": "1.11.10",
    "fast-json-stable-stringify": "2.1.0",
    "js-yaml": "4.1.0",
    "lodash": "4.17.21",
    "nunjucks": "3.2.4",
    "prompts": "2.4.2",
    "table": "6.8.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "0.5.1",
    "@types/js-yaml": "4.0.9",
    "@types/lodash": "4.14.202",
    "@types/node": "20.11.20",
    "@types/nunjucks": "3.2.6",
    "@types/prompts": "2.4.9",
    "@types/semver": "7.5.7",
    "boxen": "7.1.1",
    "chalk": "5.3.0",
    "color-json": "3.0.5",
    "eslint-config-peppy": "3.1.1",
    "eslint": "8.56.0",
    "execa": "8.0.1",
    "find-up": "7.0.0",
    "globby": "14.0.1",
    "multimatch": "7.0.0",
    "ora": "8.0.1",
    "tsup": "8.0.2",
    "typescript": "5.3.3",
    "yalc": "1.0.0-pre.53"
  }
}
